####
#   .gitignore for NeoUnimap Project
####
# Python cache / byte-compiled / optimized
__pycache__/
*.py[cod]
*$py.class

# Django/Python Environments
.env
.env~
.venv
venv/

# Deploy Directory for Apache
/deploy/*
!/deploy/.gitkeep

# Media Directory for models.ImageField
/media/*
!/media/.gitkeep
!/media/images/.gitkeep
!/media/pdf/.gitkeep
!/media/photo360/.gitkeep
!/media/photos/.gitkeep

# Logging Files
/log/*
!/log/.gitkeep

# Backup Files
/backup/*
!/backup/.gitkeep

# Mac DS_Store filesã€€
.DS_Store

# IDE eclipse/VS-Code etc.
.project
.classpath
.settings
